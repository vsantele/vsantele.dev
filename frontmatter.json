{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "series",
          "name": "series",
          "type": "choice",
          "choices": [
            "Azure Functions"
          ]
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "Azure Functions",
            "Key Vault",
            "Secrets"
          ]
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "Azure",
    "Azure Blob Storage",
    "Azure Cosmos DB",
    "Azure Functions",
    "Azure Static Web App",
    "Cognitive Services",
    "Dotnet",
    "React",
    "TypeScript"
  ],
  "frontMatter.taxonomy.categories": [
    "Job"
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.fmHighlight": true,
  "frontMatter.framework.startCommand": "hugo server -D",
  "frontMatter.preview.host": "http://localhost:1313"
}